{
  "title": "Real-time tracking of Lassa virus evolution",
  "build_url": "https://github.com/matsengrp/lassa-dmsa",
  "colorings": [
    {
      "key": "host",
      "title": "host",
      "type": "categorical"
    },
    {
      "key": "strain_original",
      "title": "strain name",
      "type": "categorical"
    },
    {
      "key": "gt",
      "title": "genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "date",
      "type": "continuous"
    },
    {
      "key": "location",
      "title": "location",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "country",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "distance_measure": "div"
  },
  "filters": [
    "host",
    "country",
    "region",
    "author"
  ]
}
